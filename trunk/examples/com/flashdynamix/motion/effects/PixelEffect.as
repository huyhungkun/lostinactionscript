package com.flashdynamix.motion.effects {	import flash.display.BitmapData;		import com.flashdynamix.motion.effects.IEffect;		/**	 * @author shanem	 */	public class PixelEffect implements IEffect {		public var color : uint;		public var x : Number;		public var y : Number;		public function PixelEffect(color : uint, x : Number,y : Number) {			this.color = color;			this.x = x;			this.y = y;		}		public function render(bmd : BitmapData) : void {			bmd.setPixel32(x, y, color);		}	}}