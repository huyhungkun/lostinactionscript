package com.flashdynamix.motion.effects {	import flash.display.BitmapData;	import flash.geom.*;	import com.flashdynamix.motion.effects.IEffect;		/**	 * @author shanem	 */	public class DissolveEffect implements IEffect {		public var speed : Number;		public var seedMin : int;		public var seedOffset : int;		public var fillColor : uint;		public var disolveBmd : BitmapData;		private var pt : Point = new Point();		function DissolveEffect(speed : Number = 10, seedMin : int = 0, seedMax : int = int.MAX_VALUE, fillColor : uint = 0x00FFFFFF, disolveBmd : BitmapData = null) {			this.speed = speed;			this.seedMin = seedMin;			this.seedOffset = seedMax - seedMin;			this.fillColor = fillColor;			this.disolveBmd = disolveBmd;		}		public function render(bmd : BitmapData) : void {			bmd.pixelDissolve((disolveBmd == null) ? bmd : disolveBmd, bmd.rect, pt, seedMin + (Math.random() * seedOffset), (bmd.width * bmd.height) / speed, fillColor);		}	}}