package com.flashdynamix.motion.effects {	import flash.display.*;		/**	 * @author shanem	 */	public class NoiseDisplacementEffect extends DisplacementEffect implements IEffect {		public var seedMin : int = 0;		public var seedOffset : int = int.MAX_VALUE;		public var low : int = 0;		public var high : int = 255;		public var grayScale : Boolean = false;		public var channelOptions : uint = BitmapDataChannel.RED;		public function NoiseDisplacementEffect(width : int = 500, height : int = 500, scaleX : Number = 3, scaleY : Number = -5) {			super(new BitmapData(width, height, true, 0x00FFFFFF), scaleX, scaleY);		}		override public function render(bmd : BitmapData) : void {			mapBmd.noise(seedMin + (seedOffset * Math.random()), low, high, channelOptions, grayScale);						super.render(bmd);		}	}}