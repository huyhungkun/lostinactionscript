package com.flashdynamix.motion.effects {	import flash.display.BitmapData;	import flash.geom.*;	import flash.utils.*;	/**	 * @author shanem	 */	public class ColorEffect extends Proxy implements IEffect {		public var ct : ColorTransform;		public function ColorEffect(ct : ColorTransform) {			this.ct = ct;		}		override flash_proxy function setProperty(name : *, value : *) : void {			ct[name] = value;		}		override flash_proxy function getProperty(name : *) : * {			return ct[name];		}		public function render(bmd : BitmapData) : void {			bmd.colorTransform(bmd.rect, ct);		}	}}