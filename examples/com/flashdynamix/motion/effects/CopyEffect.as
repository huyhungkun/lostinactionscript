package com.flashdynamix.motion.effects {	import flash.display.BitmapData;	import flash.geom.*;	import com.flashdynamix.motion.effects.IEffect;		/**	 * @author shanem	 */	public class CopyEffect implements IEffect {		public var sourceBmd : BitmapData;		public var sourceRect : Rectangle;		public var destPt : Point;		public var alphaBmd : BitmapData;		public var alphaPt : Point;		public var mergeAlpha : Boolean;		public function CopyEffect(sourceBmd : BitmapData, sourceRect : Rectangle = null, destPt : Point = null, alphaBmd : BitmapData = null, alphaPt : Point = null, mergeAlpha : Boolean = false) {			this.sourceBmd = sourceBmd;			this.sourceRect = (sourceRect == null) ? sourceBmd.rect : sourceRect;			this.destPt = (destPt == null) ? new Point() : destPt;			this.alphaBmd = alphaBmd;			this.alphaPt = alphaPt;			this.mergeAlpha = mergeAlpha;		}		public function render(bmd : BitmapData) : void {			bmd.copyPixels(sourceBmd, sourceRect, destPt, alphaBmd, alphaPt, mergeAlpha);		}	}}